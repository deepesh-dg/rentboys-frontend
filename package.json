{
  "name": "rentboys",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 3000 --host",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx}'",
    "preview": "vite preview && PORT=5555",
